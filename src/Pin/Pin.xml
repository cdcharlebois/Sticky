<?xml version="1.0" encoding="utf-8" ?>
<widget id="Pin.widget.Pin" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Pin</name>
    <description>Pin a section of your page so it sticks to the top of the page when a user scrolls.</description>



<icon></icon>




    <properties>
        <property key="selector" type="string" required="false" defaultValue="">
            <caption>Selector</caption>
            <category>General</category>
            <description>The CSS selector (or Mendix name) of the element to Pin</description>
        </property>
         <property key="selectorIsName" type="boolean" required="true" defaultValue="true">
             <caption>Is Mx Name</caption>
             <category>General</category>
             <description>Is the 'Selector' value a Mendix name?</description>
         </property>
         <property key="minWidth" type="integer" required="true" defaultValue="0">
             <caption>Minimum Width</caption>
             <category>Behavior</category>
             <description>Screens with widths below this number will not "pin" the selected element. ("0" means all devices)</description>
         </property>
         <property key="topPadding" type="integer" required="true" defaultValue="0">
             <caption>Top padding</caption>
             <category>Behavior</category>
             <description>Extra padding on top of the element (when pinned, in pixels)</description>
         </property>
         <property key="bottomPadding" type="integer" required="true" defaultValue="0">
             <caption>Bottom padding</caption>
             <category>Behavior</category>
             <description>Extra padding on bottom of the element (when pinned, in pixels)</description>
         </property>
         <property key="activeClass" type="string" required="false" defaultValue="">
             <caption>Active class</caption>
             <category>Behavior</category>
             <description>A class name to add when the element becomes pinned</description>
         </property>

    </properties>
</widget>
